{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": ["src/*"],
      "@apis/*": ["apis/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@feature/*": ["feature/*"],
      "@hooks/*": ["hooks/*"],
      "@screens/*": ["screens/*"],
      "@services/*": ["services/*"],
      "@types/*": ["types/*"],
    },
  }
}
